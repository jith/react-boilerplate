{
  "name": "react-boilerplate",
  "version": "0.1.0",
  "description": "minimal react boilerplate with webpack hot module replacement",
  "main": "index.js",
  "scripts": {
    "start": "cross-env ENV=development node ./dev.server.js",
    "build": "cross-env NODE_ENV=production webpack --config=webpack.production.config.js --progress --profile --colors"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jith/react-boilerplate.git"
  },
  "keywords": [
    "minimal",
    "react",
    "boilerplate",
    "webpack",
    "hot",
    "module",
    "replacement"
  ],
  "author": "sreejith k",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.0",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.2.7",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-hmre": "^1.1.1",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^3.1.3",
    "css-loader": "^0.25.0",
    "eslint": "^3.8.0",
    "eslint-plugin-react": "^6.7.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "^2.24.0",
    "reload-html-webpack-plugin": "^0.1.2",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.2"
  },
  "dependencies": {
    "normalize.css": "^5.0.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-router": "^3.0.0"
  }
}
