{
  "name": "react-minimal-starter-kit",
  "version": "0.2.001",
  "description": "minimal react boilerplate with webpack hot module replacement",
  "main": "index.js",
  "scripts": {
    "start": "cross-env ENV=development node ./dev.server.js",
    "build": "cross-env NODE_ENV=production webpack --config=webpack.production.config.js --progress --profile --colors"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jith/react-minimal-starter-kit.git"
  },
  "keywords": [
    "minimal",
    "react",
    "boilerplate",
    "webpack",
    "hot",
    "module",
    "replacement"
  ],
  "author": "sreejith k",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^4.0.0",
    "css-loader": "^0.28.1",
    "eslint": "^3.19.0",
    "eslint-plugin-react": "^7.0.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "html-webpack-plugin": "^2.28.0",
    "react-hot-loader": "^3.0.0-beta.7",
    "reload-html-webpack-plugin": "^0.1.2",
    "style-loader": "^0.17.0",
    "webpack": "^2.5.1",
    "webpack-dev-server": "^2.4.5"
  },
  "dependencies": {
    "normalize.css": "^7.0.0",
